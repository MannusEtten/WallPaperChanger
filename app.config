<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="flickrNet" type="FlickrNet.FlickrConfigurationManager,FlickrNet" requirePermission="false"/>
    <section name="MannusWallPaper" type="MannusWallPaper.MannusWallPaperConfiguration,MannusWallPaper" requirePermission="false"/>
  </configSections>
  <flickrNet cacheDisabled="true"/>
  <MannusWallPaper>
    <wallpapers>
      <!-- the wallpapers will be inserted in order from top to bottom -->
      <!-- the last one in this list will be the first item and the default in case Flickr is not in use -->
      <wallpaper key="company" path="esrinl.jpg" description="Esri Nederland" desktopbackcolor="White" emptydesktop="true"/>
      <wallpaper key="basketbalnieuws" path="basketbalnieuws.jpg" description="BasketBalNieuws" desktopbackcolor="Grey" emptydesktop="true"/>
    </wallpapers>
  </MannusWallPaper>
  <appSettings>
    <add key="locationofenablefile" value="c:\enablenormalmode.txt"/>
    <!-- flickr integration settings-->
    <add key="useflickrwallpaper" value="true"/>
    <add key="flickrchangetimeinminutes" value="10"/>
    <add key="flickrAPIKey" value="04b183e090d0f3eaaf84240f18a7dc2a"/>
    <add key="flickrAPISharedSecret" value="5a1315102ff1e88f"/>
    <add key="flickrUserId" value="29023370@N08"/>
    <add key="flickrSetUrl" value="http://www.flickr.com/photos/basketbalnieuws/sets/$id/"/>
    <add key="flickrPageSize" value="24"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
